<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/icon_menu_background"
    tools:context=".MainActivity" >
<LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp" >

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1" >

                <ImageButton
                    android:id="@+id/ImageButton01"
                    android:layout_width="40dp"
                    android:layout_height="45dp"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginRight="13dp"
                    android:background="@drawable/icon_logo1" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1" >

                <ImageButton
                    android:id="@+id/ImageButton1"
                    android:layout_width="40dp"
                    android:layout_height="45dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="13dp"
                    android:background="@drawable/icon_logo2" />
            </RelativeLayout>
        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="3dp"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:paddingTop="3dp" >

            <ImageView
                android:id="@+id/ImageButton2"
                android:layout_width="300dp"
                android:layout_height="44dp"
                android:src="@drawable/icon_logotext" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >

            <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/lina_paddingtop_bottom"
            android:paddingBottom="@dimen/lina_paddingtop_bottom"
            >

            <RelativeLayout
                android:layout_weight="1"
                android:padding="@dimen/menu_padding"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <ImageButton
                    android:id="@+id/ib_main_1"
                    android:layout_width="@dimen/menu_width"
                    android:layout_height="@dimen/menu_height"
                    android:onClick="btn_calculator"
                    android:background="@drawable/icon_menu_calculator" />
            </RelativeLayout>

            <RelativeLayout
                android:padding="@dimen/menu_padding"
                 android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <ImageButton
                    android:id="@+id/ib_main_2"
                    android:layout_width="@dimen/menu_width"
                    android:layout_height="@dimen/menu_height"
                    android:onClick="btn_latestnew"
                    android:background="@drawable/icon_menu_latestnew" />
            </RelativeLayout>

            <RelativeLayout
                android:padding="@dimen/menu_padding"
                 android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <ImageButton
                    android:id="@+id/ib_main_3"
                    android:layout_width="@dimen/menu_width"
                    android:layout_height="@dimen/menu_height"
                    android:onClick="btn_condition"
                    android:background="@drawable/icon_menu_condition" />
            </RelativeLayout>

            <RelativeLayout
                android:padding="@dimen/menu_padding"
                 android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >
                 <ImageButton
                    android:id="@+id/ib_main_4"
                    android:layout_width="@dimen/menu_width"
                    android:layout_height="@dimen/menu_height"
                    android:onClick="btn_process"
                    android:background="@drawable/icon_menu_process" />

            </RelativeLayout>

        </LinearLayout>

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >

            <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/lina_paddingtop_bottom"
            android:paddingBottom="@dimen/lina_paddingtop_bottom"
            >

            <RelativeLayout
                android:layout_weight="1"
                android:padding="@dimen/menu_padding"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <ImageButton
                    android:id="@+id/ib_main_5"
                    android:layout_width="@dimen/menu_width"
                    android:layout_height="@dimen/menu_height"
                    android:onClick="btn_bloodmatch"
                    android:background="@drawable/icon_menu_bloodmatch" />
            </RelativeLayout>

            <RelativeLayout
                android:padding="@dimen/menu_padding"
                 android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <ImageButton
                    android:id="@+id/ib_main_6"
                    android:layout_width="@dimen/menu_width"
                    android:layout_height="@dimen/menu_height"
                    android:onClick="btn_sound"
                    android:background="@drawable/icon_menu_sound" />
            </RelativeLayout>

            <RelativeLayout
                android:padding="@dimen/menu_padding"
                 android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <ImageButton
                    android:id="@+id/ib_main_7"
                    android:layout_width="@dimen/menu_width"
                    android:layout_height="@dimen/menu_height"
                    android:onClick="btn_chengfenjuanxue"
                    android:background="@drawable/icon_menu_chengfenjuanxue" />
            </RelativeLayout>

            <RelativeLayout
                android:padding="@dimen/menu_padding"
                 android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >
                 <ImageButton
                    android:id="@+id/ib_main_8"
                    android:layout_width="@dimen/menu_width"
                    android:layout_height="@dimen/menu_height"
                    android:onClick="btn_friends"
                    android:background="@drawable/icon_menu_friends" />

            </RelativeLayout>

        </LinearLayout>

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >

            <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/lina_paddingtop_bottom"
            android:paddingBottom="@dimen/lina_paddingtop_bottom"
            >

            <RelativeLayout
                android:layout_weight="1"
                android:padding="@dimen/menu_padding"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <ImageButton
                    android:id="@+id/ib_main_9"
                    android:layout_width="@dimen/menu_width"
                    android:layout_height="@dimen/menu_height"
                    android:onClick="btn_home"
                    android:background="@drawable/icon_menu_home" />
            </RelativeLayout>

            <RelativeLayout
                android:padding="@dimen/menu_padding"
                 android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <ImageButton
                    android:id="@+id/ib_main_10"
                    android:layout_width="@dimen/menu_width"
                    android:layout_height="@dimen/menu_height"
                    android:onClick="btn_station"
                    android:background="@drawable/icon_menu_station" />
            </RelativeLayout>

            <RelativeLayout
                android:padding="@dimen/menu_padding"
                 android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <ImageButton
                    android:id="@+id/ib_main_11"
                    android:layout_width="@dimen/menu_width"
                    android:layout_height="@dimen/menu_height"
                    android:onClick="btn_donate"
                    android:background="@drawable/icon_menu_donate" />
            </RelativeLayout>

            <RelativeLayout
                android:padding="@dimen/menu_padding"
                 android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >
                 <ImageButton
                    android:id="@+id/ib_main_12"
                    android:layout_width="@dimen/menu_width"
                    android:layout_height="@dimen/menu_height"
                    android:onClick="btn_introduce"
                    android:background="@drawable/icon_introduce" />

            </RelativeLayout>

        </LinearLayout>

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >

            <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/lina_paddingtop_bottom"
            android:paddingBottom="@dimen/lina_paddingtop_bottom"
            >

            <RelativeLayout
                android:layout_weight="1"
                android:padding="@dimen/menu_padding"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <ImageButton
                    android:id="@+id/ib_main_13"
                    android:layout_width="@dimen/menu_width"
                    android:layout_height="@dimen/menu_height"
                    android:onClick="btn_question"
                    android:background="@drawable/icon_menu_question" />
            </RelativeLayout>

            <RelativeLayout
                android:padding="@dimen/menu_padding"
                 android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <ImageButton
                    android:id="@+id/ib_main_14"
                    android:layout_width="@dimen/menu_width"
                    android:layout_height="@dimen/menu_height"
                    android:onClick="btn_downloadtable"
                    android:background="@drawable/icon_menu_downloadtable" />
            </RelativeLayout>

            <RelativeLayout
                android:padding="@dimen/menu_padding"
                 android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <ImageButton
                    android:id="@+id/ib_main_15"
                    android:layout_width="@dimen/menu_width"
                    android:layout_height="@dimen/menu_height"
                    android:onClick="btn_contactus"
                    android:background="@drawable/icon_menu_contactus" />
            </RelativeLayout>

            <RelativeLayout
                android:padding="@dimen/menu_padding"
                 android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >
                 <ImageButton
                    android:id="@+id/ib_main_16"
                    android:layout_width="@dimen/menu_width"
                    android:layout_height="@dimen/menu_height"
                    android:onClick="btn_setting"
                    android:background="@drawable/icon_menu_setting" />

            </RelativeLayout>

        </LinearLayout>

        </RelativeLayout>

    </LinearLayout>

</RelativeLayout>
